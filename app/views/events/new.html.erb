<%= form_with model: @event do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.label :date %>
    <%= f.date_field :date%>
  </div>

  <%= f.submit :Submit %>
<% end %>
